  "volta": {
    "node": "22.21.0"
  },
  "dependencies": {
    "@prisma/client": "^6.4.0",
    "bree": "^9.2.0",
    "cbor": "^9.0.0",
    "chokidar": "^4.0.0",
    "cors": "^2.8.5",
    "dotenv-cli": "^8.0.0",
    "express": "^4.21.0",
    "lodash": "^4.17.0",
    "lowdb": "^6.1.0",
    "moment-timezone": "^0.5.0",
    "node-schedule": "^2.1.0",
    "winston": "^3.17.0",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.0",
    "@types/cors": "^2.8.0",
    "@types/express": "^5.0.0",
