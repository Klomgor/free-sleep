    "@types/node-schedule": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.0",
    "nodemon": "^3.1.0",
    "prisma": "^6.4.0",
    "ts-essentials": "^10.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.7.0"
  }
}
